.page-header
  %h1<
    &=params[:q]
    %small<
      =@subtitle
      
- if @items.length > 0
  #search-results
    - @items.each do |item|
      %p.lead.search-result<
        =item.description
        %br
        =item.time_block.title
        &nbsp;
        %a=item.meta['pdf_info']['filename']
    
    